set positional-arguments
set fallback := true

set shell := ["bash", "-cue"]
root_dir := justfile_directory()

api-store-key:
    cd "{{root_dir}}" && \
        http PUT http://localhost:8080/api/v1/demo/keys/$(uuidgen) \
            public_key=asdf \
            private_key_encrypted=asdf


run: api-store-key
